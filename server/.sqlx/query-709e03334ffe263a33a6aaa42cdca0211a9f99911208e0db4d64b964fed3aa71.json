{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                transfers.id,\n                transfers.created_at,\n                transfers.last_modification_at,\n                transfers.maker_id,\n                transfers.taker_id,\n                transfers.asset_id,\n                transfers.amount as \"amount: Fraction\"\n            FROM transfers\n            WHERE transfers.taker_id = $1 OR transfers.maker_id = $1\n            ORDER BY transfers.created_at DESC\n            LIMIT $2\n            OFFSET $3\n            ",
